{
  "name": "kisaragi",
  "version": "1.0.0",
  "description": "A kawaii discord bot",
  "main": "dist/shard.js",
  "scripts": {
    "start": "npm run build && npm run bot",
    "server-start": "npm run build && npm run server",
    "build": "tsc -p tsconfig.json && node dist/post.js",
    "bot": "node --max-old-space-size=8192 --optimize-for-size dist/shard.js",
    "server": "node --max-old-space-size=8192 --optimize-for-size dist/server.js",
    "pm2": "pm2 start ecosystem.config.js --only kisaragi-bot --env production",
    "test": "ts-mocha --timeout 10000 -p tsconfig.json test/**/*.spec.ts",
    "register": "npm run build && cross-env REDIS=off node dist/register.js",
    "generate": "npm run build && node dist/generate.js",
    "ssh": "dotenv -e .env -- sh -c 'ssh -i \"$SSH_KEY_PATH\" \"$SSH_USER\"@\"$SSH_HOST\"'",
    "clean": "del-cli dist"
  },
  "engines": {
    "node": "^23.7.0",
    "npm": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Moebytes/Kisaragi.git"
